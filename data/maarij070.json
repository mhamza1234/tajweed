        {
          "ar": "وَتَكُونُ",
          "tr": "watakūnu",
          "wbw_audio": "wbw/070_009_001.mp3",
          "rules": []
        },
        {
          "ar": "ٱلْجِبَالُ",
          "tr": "l-jibālu",
          "wbw_audio": "wbw/070_009_002.mp3",
          "rules": []
        },
        {
          "ar": "كَٱلْعِهْنِ",
          "tr": "kal-ʿih'ni",
          "wbw_audio": "wbw/070_009_003.mp3",
          "rules": []
        }
      ]
    },
    {
      "ayah_id": "70:10",
      "arabic_tajweed_html": "وَلَا يَسْــَٔلُ حَمِيمٌ حَمِيمًا <span class=end>١٠</span>",
