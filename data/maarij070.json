  "verses": [
    {
      "ayah_id": "70:1",
      "arabic_tajweed_html": "سَأَلَ س<tajweed class=madda_obligatory>َا</tajweed>ٓئِ<tajweed class=iqlab>لُۢ ب</tajweed>ِعَذَا<tajweed class=idgham_ghunnah>بٍ و</tajweed>َاقِعٍ <span class=end>١</span>",
      "bangla": "A man [mockingly] demanded the punishment.",
      "words": [
        {
          "ar": "سَأَلَ",
          "tr": "sa-ala",
          "wbw_audio": "wbw/070_001_001.mp3",
          "rules": []
        },
        {
