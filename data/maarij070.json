      "words": [
        {
          "ar": "مِّنَ",
          "tr": "mina",
          "wbw_audio": "wbw/070_003_001.mp3",
          "rules": []
        },
        {
          "ar": "ٱللَّهِ",
          "tr": "l-lahi",
          "wbw_audio": "wbw/070_003_002.mp3",
          "rules": []
        },
        {
          "ar": "ذِى",
          "tr": "dhī",
