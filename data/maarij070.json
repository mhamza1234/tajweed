        {
          "ar": "وَاقِعٍۢ",
          "tr": "wāqiʿin",
          "wbw_audio": "wbw/070_001_004.mp3",
          "rules": []
        }
      ]
    },
    {
      "ayah_id": "70:2",
      "arabic_tajweed_html": "لِّلْكَ<tajweed class=madda_normal>ـٰ</tajweed>فِرِينَ لَيْسَ لَهُ<tajweed class=madda_normal>ۥ</tajweed> دَافِعٌ <span class=end>٢</span>",
      "bangla": "It will fall on the disbelievers––none can deflect it––",
