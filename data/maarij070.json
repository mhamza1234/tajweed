    {
      "ayah_id": "70:5",
      "arabic_tajweed_html": "فَ<tajweed class=ham_wasl>ٱ</tajweed>صْبِرْ صَ<tajweed class=qalaqah>بْ</tajweed><tajweed class=ikhafa>رًا ج</tajweed>َمِيلاً <span class=end>٥</span>",
      "bangla": "So be patient, [Prophet], as befits you.",
      "words": [
        {
          "ar": "فَٱصْبِرْ",
          "tr": "fa-iṣ'bir",
          "wbw_audio": "wbw/070_005_001.mp3",
          "rules": []
        },
        {
          "ar": "صَبْرًۭا",
          "tr": "ṣabran",
          "wbw_audio": "wbw/070_005_002.mp3",
          "rules": []
        },
        {
          "ar": "جَمِيلًا",
          "tr": "jamīlan",
          "wbw_audio": "wbw/070_005_003.mp3",
          "rules": []
        }
      ]
    },
    {
      "ayah_id": "70:6",
      "arabic_tajweed_html": "إِ<tajweed class=ghunnah>نّ</tajweed>َهُمْ يَرَوْنَهُ<tajweed class=madda_normal>ۥ</tajweed> بَعِيدًا <span class=end>٦</span>",
      "bangla": "The disbelievers think it is distant,",
      "words": [
        {
          "ar": "إِنَّهُمْ",
          "tr": "innahum",
          "wbw_audio": "wbw/070_006_001.mp3",
          "rules": []
        },
        {
          "ar": "يَرَوْنَهُۥ",
          "tr": "yarawnahu",
          "wbw_audio": "wbw/070_006_002.mp3",
          "rules": []
        },
        {
          "ar": "بَعِيدًۭا",
          "tr": "baʿīdan",
          "wbw_audio": "wbw/070_006_003.mp3",
          "rules": []
        }
      ]
    },
    {
      "ayah_id": "70:7",
      "arabic_tajweed_html": "وَنَرَ<tajweed class=madda_normal>ٮٰ</tajweed>هُ قَرِيبًا <span class=end>٧</span>",
      "bangla": "but We know it to be close.",
