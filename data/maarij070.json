          "tr": "lil'kāfirīna",
          "wbw_audio": "wbw/070_002_001.mp3",
          "rules": []
        },
        {
          "ar": "لَيْسَ",
          "tr": "laysa",
          "wbw_audio": "wbw/070_002_002.mp3",
          "rules": []
        },
        {
          "ar": "لَهُۥ",
          "tr": "lahu",
          "wbw_audio": "wbw/070_002_003.mp3",
          "rules": []
